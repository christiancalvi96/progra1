#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()

{
	float b,a,area;
	cout<< "ingrese base del triangulo:";
	cin>>b;
	cout<< "ingrese la altura del triangulo:";
	cin>>a;
	area=(b*a)/2;
	cout<< "el area es:"<<area;
	getch();
}
