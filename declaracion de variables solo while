// validacion de datos.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main ()

{
	int n,i,p=1;
	do {
		cout<< "ingrese un numero: "<<endl;
		cin>> n;
	} while (n<0);
       i=1;
	   while (i<=n)
	   {
		   p=p*1;
		   i++;
	   }

	cout<< "el factor es: "<<p<<endl;
	getch();

}
